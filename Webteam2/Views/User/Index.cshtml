
@{
    ViewData["Title"] = "Index";

}

<br />
<h2 class="text-info">Users List</h2>
<br />

<div class="container row p-0 m-0">
    <div class="col-12 border p-3 mt-3">
        <form action="/" method="post">
            <table id="users_table" class="table table-striped border">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Reputation</th>
                        <th>Tel</th>
                        <th>Location</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Row 1 First Name</td>
                        <td>Row 1 Last Name</td>
                        <td>Row 1 Reputation</td>
                        <td>Row 1 Tel</td>
                        <td>Row 1 Location</td>
                    </tr>
                </tbody>
            </table>
        </form>
    </div>
</div>
@section scripts{
    <script src="~/js/UsersList.js"></script>
}